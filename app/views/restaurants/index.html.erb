<div class="container">
<h1 style="text-align: center">Carlos' Restaurants</h1>
  <% @restaurants.each do |restaurant| %>
  <h1><%= link_to restaurant.name, restaurant_path(restaurant) %></h1>
  <h3><%= restaurant.address %></h3>
  <h3><%= restaurant.phone_number %></h3>
  <h3><%= restaurant.category.capitalize %></h3>
  <% end %>

  <h3><%= link_to "+Add a New Restaurant", new_restaurant_path %></h3>
</div>
