<div class="container">
  <%= simple_form_for(@restaurant) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number%>
    <%= f.input :category, as: :select, collection: ["italian", "chinese", "japanese", "french", "belgian"] %>
    <%= f.submit %>
  <% end %>
</div>
